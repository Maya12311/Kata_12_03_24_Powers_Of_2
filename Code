public class Kata{
  public static long[] powersOfTwo(int n){
    long[] newArray = new long[n+1]; 
    long num = 1; 
    for(int i = 0 ; i<= n; i++){
      
      if(i== 0) {
        newArray[i] = 1;
        }
      else{
        num= num*2;
        newArray[i] = num;
      }
      
    }
    return newArray;
  }
}
